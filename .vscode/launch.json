{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}",
      "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v18.13.0/bin/node",
      "preLaunchTask": "Clear terminal",
      "console": "integratedTerminal"
    }
  ]
}
